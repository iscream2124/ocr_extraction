{
  "version": 2,
  "name": "mineru-gui",
  "builds": [
    {
      "src": "web-version/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web-version/$1"
    }
  ],
  "functions": {
    "api/process.js": {
      "runtime": "nodejs18.x"
    }
  }
}
